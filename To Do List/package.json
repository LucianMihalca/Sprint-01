{
  "name": "to-do-list",
  "version": "1.0.0",
  "description": "A simple to-do list application built with TypeScript and SASS.",
  "main": "index.js",
  "scripts": {
    "start": "tsc --watch",
    "start:dev": "nodemon dist",
