    "test": "your-test-runner command"
  },
  "keywords": ["to-do", "TypeScript", "SASS", "task management"],
  "author": "Lucian",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "your-test-runner": "^version"
  },
  "dependencies": {
    "sass": "^1.69.0"
  }
}
